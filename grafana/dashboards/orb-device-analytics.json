        {
          "datasource": {
            "type": "influxdb",
            "uid": "orb-influxdb"
          },
          "query": "import \"date\"\n\ndata = from(bucket: \"orb_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"scores\")\n  |> filter(fn: (r) => r[\"_field\"] == \"orb_score\")\n  |> filter(fn: (r) => r[\"orb_name\"] == \"${orb_device}\")\n  |> map(fn: (r) => ({r with outage: if r._value < 50 then 1 else 0}))\n  |> difference(nonNegative: false, columns: [\"outage\"])\n  |> filter(fn: (r) => r.outage != 0)\n  |> map(fn: (r) => ({r with event: if r.outage > 0 then \"start\" else \"end\"}))\n  |> group()\n  |> sort(columns: [\"_time\"])\n  \nstarts = data |> filter(fn: (r) => r.event == \"start\") |> findRecord(fn: (key) => true, idx: 0)\nends = data |> filter(fn: (r) => r.event == \"end\") |> findRecord(fn: (key) => true, idx: 0)\n\nif exists starts._time and exists ends._time then\n  union(tables: [\n    from(bucket: \"orb_metrics\") |> range(start: starts._time, stop: ends._time) |> limit(n: 1)\n      |> map(fn: (r) => ({\n        \"Start Time\": starts._time,\n        \"End Time\": ends._time,\n        \"Duration (minutes)\": float(v: uint(v: ends._time) - uint(v: starts._time)) / 60000000000.0\n      }))\n  ])\nelse\n  from(bucket: \"orb_metrics\") |> range(start: -1s) |> limit(n: 0)\n    |> map(fn: (r) => ({\n      \"Start Time\": 0,\n      \"End Time\": 0,\n      \"Duration (minutes)\": 0.0\n    }))",
          "refId": "A"
        }
      ],
      "title": "Outages (Score < 50)",
      "type": "table"
    }
  ],
