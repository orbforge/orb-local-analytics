                    "query": "scores = from(bucket: \"orb_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"scores\" and r._field == \"orb_score\")\n  |> group(columns: [\"city\", \"country_code\"])\n  |> mean()\n  |> map(fn: (r) => ({city: r.city, country: r.country_code, score: r._value}))\n\nlatency = from(bucket: \"orb_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"responsiveness\" and r._field == \"latency_avg_us\")\n  |> group(columns: [\"city\"])\n  |> mean()\n  |> map(fn: (r) => ({city: r.city, latency: r._value}))\n\ndevice_count = from(bucket: \"orb_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"scores\" and r._field == \"orb_score\")\n  |> group(columns: [\"city\", \"orb_name\"])\n  |> count()\n  |> group(columns: [\"city\"])\n  |> count()\n  |> map(fn: (r) => ({city: r.city, count: r._value}))\n\njoin1 = join(tables: {t1: scores, t2: latency}, on: [\"city\"])\njoin2 = join(tables: {t1: join1, t2: device_count}, on: [\"city\"])\n  |> map(fn: (r) => ({\n    \"City\": r.city,\n    \"Country\": r.country,\n    \"Avg Score\": r.score,\n    \"Avg Latency\": r.latency,\n    \"Device Count\": r.count\n  }))",
